{
  "name": "hannas-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://demo.getfirefly.org",
  "dependencies": {
    "env-cmd": "^10.1.0",
    "firebase": "^6.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-firestore": "^0.8.1",
    "react-ga": "^2.5.3",
    "react-instantsearch-dom": "^5.2.3",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^3.2.0",
    "react-transition-group": "^2.3.1",
    "slugify": "^1.3.0",
    "styled-components": "^3.3.2"
  },
  "scripts": {
    "": "How these scripts work: https://medium.com/@tacomanator/environments-with-create-react-app-7b645312c09d",
    "start": "env-cmd -f .env.development firebase use dev && react-scripts start",
    "start:staging": "env-cmd -f .env.staging firebase use stage && react-scripts start",
    "build": "firebase use prod && react-scripts build",
    "build:staging": "env-cmd -f .env.staging firebase use staging && react-scripts build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
